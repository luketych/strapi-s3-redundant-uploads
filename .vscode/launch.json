{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Install Dependencies",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["install"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Start Development Server",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "develop"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal"
        }
    ]
}
